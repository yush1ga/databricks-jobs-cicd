{
    "name": "databricks-jobs-cicd-sample",
    "email_notifications": {
      "no_alert_for_skipped_runs": false
    },
    "webhook_notifications": {},
    "timeout_seconds": 0,
    "max_concurrent_runs": 1,
    "tasks": [
      {
        "task_key": "job01",
        "run_if": "ALL_SUCCESS",
        "spark_python_task": {
          "python_file": "./job01.py",
          "source": "GIT"
        },
        "timeout_seconds": 0,
        "email_notifications": {},
        "notification_settings": {
          "no_alert_for_skipped_runs": false,
          "no_alert_for_canceled_runs": false,
          "alert_on_last_attempt": false
        },
        "webhook_notifications": {},
        "environment_key": "Default"
      },
      {
        "task_key": "job02",
        "depends_on": [
          {
            "task_key": "job01"
          }
        ],
        "run_if": "ALL_SUCCESS",
        "spark_python_task": {
          "python_file": "./job02.py",
          "source": "GIT"
        },
        "timeout_seconds": 0,
        "email_notifications": {},
        "notification_settings": {
          "no_alert_for_skipped_runs": false,
          "no_alert_for_canceled_runs": false,
          "alert_on_last_attempt": false
        },
        "webhook_notifications": {},
        "environment_key": "Default"
      }
    ],
    "format": "MULTI_TASK",
    "queue": {
      "enabled": true
    },
    "environments": [
      {
        "environment_key": "Default",
        "spec": {
          "client": "1"
        }
      }
    ]
}